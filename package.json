{
  "name": "evan-lang",
  "version": "0.0.1",
  "description": "A vaguely Lispy language in the context of JavaScript.",
  "main": "interpreter.js",
  "scripts": {
    "postinstall": "tsd reinstall && mkdir -p test/actual/evaluation",
    "build": "tsc",
    "prebuild": "npm run build-meta && tsconfig",
    "test": "node dist/test/test-all.js && diff --recursive test/actual/ test/expected/",
    "pretest": "npm run build",
    "start": "http-server -p 8031 ide/web",
    "prestart": "npm run build-web",
    "build-web": "webpack",
    "prebuild-web": "npm run build",
	"build-meta": "node meta/build-meta.js"
  },
  "author": "Meinte Boersma",
  "license": "MIT",
  "dependencies": {
    "http-server": "^0.9.0",
    "mobx": "^2.3.5",
    "mobx-react": "^3.5.0",
    "nscript": "^0.1.7",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "camelcase": "^3.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.14.0",
    "node-debug": "^0.1.0",
    "path": "^0.12.7",
    "ts-loader": "^0.8.2",
    "tsconfig-glob": "^0.4.3",
    "tsd": "^0.6.5",
    "typescript": "^1.8.10",
    "webpack": "^1.13.1"
  }
}
